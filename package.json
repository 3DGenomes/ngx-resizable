{
  "name": "ngx-resizable-demo",
  "version": "1.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build_lib": "ng build ngx-resizable",
    "copy-license": "cp LICENSE.txt dist/ngx-resizable",
    "copy-readme": "cp README.md dist/ngx-resizable",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-resizable && npm pack",
    "demo2docs": "ng build --base-href https://3dgenomes.github.io/ngx-resizable/ --output-path docs",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack && npm run demo2docs",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@3dgenomes/ngx-resizable": "^1.8.1",
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "core-js": "^2.6.11",
    "rxjs": "^6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^11.15.4",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "ng-packagr": "^5.7.1",
    "protractor": "^5.4.2",
    "ts-node": "^8.5.4",
    "tsickle": "^0.35.0",
    "tslib": "^1.10.0",
    "tslint": "~5.12.1",
    "typescript": "~3.4.5"
  }
}
